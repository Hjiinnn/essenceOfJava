package essence.ch6;

class Data3 {
	int value;
}

class Data2 {
	int value;
	
	// 매개변수가 있는 생성자.
	Data2(int x) {
		value = x;
	}
}

class ConstructorTest {

	public static void main(String[] args) {
		Data1 d1 = new Data1();
		//Data2 dw = new Data2();		// ERROR!!! compile error 발생 (The constructor Data2() is undefined)

	}

}
